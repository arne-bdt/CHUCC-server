{
  "$schema": "https://json-schema.org/draft/2020-12/schema",
  "$id": "https://example.org/sparql-vc/create-branch.json",
  "type": "object",
  "required": ["name"],
  "properties": {
    "name": { "type": "string", "pattern": "^[A-Za-z0-9._\\-]+$" },
    "from": { "type": "string", "description": "commit UUIDv7 or branch name" }
  }
}
